# Newtonâ€™s Divided Difference Method
def linear(x, t, v):
  b = [None]*len(t)
  b[0] = v[0]
  b[1] = (v[1] - v[0]) / (t[1] - t[0])

  res = b[0] + b[1]*(x - t[0])
  return res;

def quadratic(x, t, v):
  b = [None]*len(t)
  b[0] = v[0]
  b[1] = (v[1] - v[0]) / (t[1] - t[0])

  b[2] = (v[2] - v[1]) / (t[2] - t[1]) - (v[1] - v[0]) / (t[1] - t[0])
  b[2] /= (t[2] - t[0])

  res = b[0] + b[1]*(x - t[0]) + b[2]*(x - t[0])*(x - t[1])
  return res;

def cubic(x, t, v):
  b = [None]*len(t)
  b[0] = v[0]
  b[1] = (v[1] - v[0]) / (t[1] - t[0])

  b[2] = (v[2] - v[1]) / (t[2] - t[1]) - b[1]
  b[2] /= (t[2] - t[0])

  b[3] = (v[3] - v[2]) / (t[3] - t[2]) - (v[2] - v[1]) / (t[2] - t[1])
  b[3] /= (t[3] - t[1])

  b[3] -= b[2]
  b[3] /= (t[3] - t[0])

  res = b[0] + b[1]*(x - t[0]) + b[2]*(x - t[0])*(x - t[1]) + b[3]*(x - t[0])*(x - t[1])*(x - t[2])
  return res;

